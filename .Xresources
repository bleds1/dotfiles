!! Transparency (0-1):
*.alpha: 0.9

Xft.dpi:       96
Xft.antialias: true
Xft.hinting:   true
Xft.rgba:      rgb
Xft.autohint:  false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault

XTerm*background:        #222D31
XTerm*foreground:        #d8d8d8
XTerm*pointerColor:      #1ABB9B
XTerm*faceName:          Fixed
XTerm*faceSize:          11
XTerm*reverseVideo:      on
XTerm*selectToClipboard: true

*background:                      #272727
*foreground:                      #d8d8d8
*fading:                          8
*fadeColor:                       black
*cursorColor:                     #1ABB9B
*pointerColorBackground:          #2B2C2B
*pointerColorForeground:          #16A085

!! black dark/light
*color0:                          #222D31
*color8:                          #585858

!! red dark/light
*color1:                          #ab4642
*color9:                          #ab4642

!! green dark/light
*color2:                          #7E807E
*color10:                         #8D8F8D

!! yellow dark/light
*color3:                          #FFF7AD
*color11:                         #FFD7AD

!! blue dark/light
*color4:                          #7cafc2
*color12:                         #7cafc2

!! magenta dark/light
*color5:                          #ba8baf
*color13:                         #ba8baf

!! cyan dark/light
*color6:                          #1ABB9B
*color14:                         #1ABB9B

!! white dark/light
*color7:                          #d8d8d8
*color15:                         #f8f8f8

Xcursor.theme: pixelfun3
Xcursor.size:                     0

!URxvt-Settings
xterm*termName: xterm-256colors
URxvt*termName: rxvt-unicode-256color
!URxvt.font:                       9x15,xft:terminus
! alternative font settings with 'terminus':
!!! -xos4-terminus-medium-r-normal--12-120-72-72-c-60-iso10646-1, \
!URxvt.font: xft:Iosevka:style=Regular:size=10
!xft:mononoki Nerd Font:style=UniqueID:pixelsize=8
!!URxvt.boldFont: xft:Iosevka:style=Bold:size=10
!!URxvt.italicFont: xft:Iosevka:style=Italic:size=8
!!URxvt.boldItalicfont: xft:Iosevka:style=Bold Italic:size=8
URxvt.letterSpace: 0
URxvt.lineSpace: 0
URxvt.geometry: 92x24
URxvt.internalBorder: 8
!! terminus names see end of file!

URxvt.font:     xft:Iosevka:style=Regular:size=10, \
!		-xos4-terminus-medium-r-normal--12-120-72-72-c-60-iso10646-1, \
		xft:mononoki Nerd Font:style=UniqueID:pixelsize=10, \
Xft.lcdfilter: lcdfilter
URxvt*imLocale: en_US.UTF-8
URxvt.intensityStyles: false
URxvt.depth:                      32
URxvt.background:                 [100]#272727
URxvt*scrollBar:                  false
URxvt*scrollBar_right: false
URxvt*mouseWheelScrollPage:       true
URxvt*cursorBlink:                true
URxvt*background:                 black
URxvt*foreground:                 grey
URxvt*saveLines:                  5000
Urxvt.title: 			  urxvt	
! for 'fake' transparency (without Compton) uncomment the following three lines
URxvt*inheritPixmap:true
URxvt*transparent: false
URxvt*shading: 138
! prevent some kind of warning?
URxvt.iso14755: false
URxvt.iso14755_52: false
! extensions and keybinds
URvxt.perl-ext-common: default,clipboard,url-select,resize-font, keyboard-select
URxvt.url-select.launcher: firefox
URxvt.url-select.underline: true
URxvt.colorUL: #4682B4
URxvt.keysym.M-Escape: builtin-string:
URxvt.keysym.M-Escape: perl:keyboard-select:activate
! copy-paste
! Disable
URxvt.keysym.M-p: builtin-string:
URxvt.keysym.M-y: builtin-string:
URxvt.keysym.M-u: builtin-string:
URxvt.keysym.M-z: builtin-string:
URxvt.keysym.M-x: builtin-string:
URxvt.keysym.M-c: builtin-string:
URxvt.keysym.M-j: builtin-string:
URxvt.keysym.M-k: builtin-string:
! Enable
URxvt.keysym.M-p: eval:paste_clipboard
URxvt.keysym.M-y: eval:selection_to_clipboard
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.keysym.M-j: command:\033]721;1\007
URxvt.keysym.M-k: command:\033]720;1\007
! url matcher
URxvt.matcher.button: 1
! Xterm escape codes, word by word movement
URxvt.keysym.Control-Left: \033[1;5D
URxvt.keysym.Shift-Control-Left: \033[1;6D
URxvt.keysym.Control-Right: \033[1;5C
URxvt.keysym.Shift-Control-Right: \033[1;6C
! Disable up down 
! Enable altjk up down
URxvt.keysym.Shift-Up: command:\033]720;1\007
URxvt.keysym.Shift-Down: command:\033]721;1\007
URxvt.keysym.M-z:    command:\033]710;xft:Iosevka:style=Regular:size=9\007
URxvt.keysym.M-x:    command:\033]710;xft:Iosevka:style=Regular:size=10\007
URxvt.keysym.M-c:    command:\033]710;xft:Iosevka:style=Regular:size=11\007
! URxvt.keysym.M-z: resize-font:smaller
! URxvt.keysym.M-x: resize-font:bigger
! URxvt.keysym.M-c: resize-font:reset
! URxvt.keysym.C-question:  resize-font:show

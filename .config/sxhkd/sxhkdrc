super + Escape
        pkill =USR1 -x sxhkd

# Launching Programs
# Terminal (Keeping this one in i3 just in case)
#super + t
#        kitty
# Open GUI Programs
super + o ; {b,f,n,v,k,s,g,m,o}
        {qutebrowser,firefox,nautilus,freetube,krita,steam,gpick,audacious,obsidian}
# Ranger
super + period; period
       kitty -e ranger
super + period; i
       kitty -e ranger ~/Dropbox/0_Inbox
super + period; r
       kitty -e ranger ~/Dropbox/roam
super + period; p
       kitty -e ranger ~/Dropbox/roam/pages
super + period; d
       kitty -e ranger ~/Downloads
super + period; l
       kitty -e ranger ~/Documents/Library/pdf
# Rofi
# Run Launcher
super + x
       rofi -show run
# Bookmarks web
super + Return
       bookmarks-rofi
# Bluetooth rofi
super b; t
      rofi-bluetooth
# Window swticher
super + comma
       rofi -show window
# Clipboard
super + c
       rofi -modi "clipboard:greenclip print" -show clipboard -run-command '{cmd}'
# Searchweb
super + slash
      searchweb
# Pass
super + alt + p
        dmenu_pass
# Screenshots
Print
    xfce4-screenshooter

super + shift; p
      xfce4-screenshooter
# Screenlock
super +z
      xflock4

# Emacs (super + e followed by another key)
super + e; e
    emacsclient -c -a 'emacs'
super + b; i
    emacsclient -c -a 'emacs' --eval '(ibuffer)'
super + e; d
    emacsclient -c -a 'emacs' --eval '(dired nil)'
super + e; f
    emacsclient -c -a 'emacs' --eval '(elfeed)'
super + e; m
    emacsclient -c -a 'emacs' --eval '(my-mu4e-all-mail)'
super + e; c
    emacsclient -c -a 'emacs' --eval '(compose-mail)'
super + e; n
    emacsclient -c -a 'emacs' --eval '(org-roam-dailies-goto-today)'
super + e; t
    emacsclient -c -a 'emacs' ~/Dropbox/roam/tasks.org
super + e; i
    emacsclient -c -a 'emacs' ~/Dropbox/roam/tasks.org
# i3 windows
super +w; c
 i3-msg kill
